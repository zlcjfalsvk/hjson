{
  # comments are treated like whitespace - they are not parsed

  hello: "Hello Human!",

  // js style comment

  /*
  multiline comment
  multiline comment
  */

  # text
  text: "This is a valid string value.",
  quote: "You need quotes\tfor escapes",
  otherwise: "<div class=\"cool\">life without escapes is bliss!</div>",

  # keys
  abc-123: "no quotes for keys",

  # comma
  commas: "can be omitted at the end of the line",
  but:
  [
    1,
    2,
    3
  ], # not between elements on the same line
  trailing:
  [
    1,
    2,
    3
  ], # a trailing comma is OK

  # multiline string
  multiline: "first line\n  indented line\nthird line",

  # numbers, keywords and arrays work just like in JSON
  number: 5,
  negative: -4.2,
  yes: true,
  no: false,
  null: null,
  array:
  [
    1,
    2
  ],
  array2:
  [
    1,
    2
  ],
  children: {
    id: "1234567",
    # cheolcheol comment
    name: "cheolcheol"
  }
}